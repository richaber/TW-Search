{
  "name": "twister-mc/tw-search",
  "description": "A WordPress plugin that adds a simple search overlay. No need to worry about design.",
  "authors": [
    {
      "name": "Thomas McMahon",
      "homepage": "https://www.twistermc.com",
      "role": "Lead Developer"
    },
    {
      "name": "Richard Aber",
      "homepage": "https://richaber.com",
      "role": "Contributor"
    }
  ],
  "require": {
    "php": ">=7.1",
    "league/container": "^3.3",
    "psr/container": "^1.0",
    "psr/container-implementation": "^1.0",
    "typisttech/imposter-plugin": "^0.3.1",
    "typisttech/wp-contained-hook": "^0.3.0",
    "gamajo/template-loader": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "TwisterMc\\TWSearch\\": "src/"
    }
  },
  "extra": {
    "imposter": {
      "namespace": "TwisterMc\\TWSearch\\Vendor",
      "excludes": [
        "psr/container-implementation"
      ]
    }
  }
}
